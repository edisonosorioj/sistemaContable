ALTER TABLE `clientes` ADD COLUMN `fecha_nacimiento` VARCHAR(45) NULL AFTER `direccion`, ADD COLUMN `doc_empresa` VARCHAR(45) NULL  AFTER `fecha_nacimiento`, ADD COLUMN `rh` VARCHAR(45) NULL AFTER `doc_empresa`, ADD COLUMN `categoria` VARCHAR(45) NULL AFTER `rh`, ADD COLUMN `seguro_social` VARCHAR(45) NULL AFTER `categoria`;

ALTER TABLE `clientes` ADD COLUMN `estado` VARCHAR(45) NOT NULL DEFAULT 0 AFTER `seguro_social`;